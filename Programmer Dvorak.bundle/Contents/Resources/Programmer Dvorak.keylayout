		</action>
		<action id="compose">
			<when state="none" next="compose"/>
		</action>
		<action id="d">
			<when state="none" output="d"/>
			<when state="compose" next="eth"/>
		</action>
		<action id="dash">
			<when state="none" output="-"/>
			<when state="compose" next="macron"/>
			<when state="dot" output="·"/>
